<label
  pos="{{pos.x}} {{pos.y}}"
  z-index="{{zIndex}}"
  scale="{{scale}}"
  rot="{{rot}}"
  hidden="{{boolToNum hidden}}"
  id="{{id}}"
  class="{{class}}"
  size="{{size.width}} {{size.height}}"
  halign="{{hAlign}}"
  valign="{{vAlign}}"
  scriptevents="{{boolToNum scriptEvents}}"
  opacity="{{boolToNum opacity}}"
  action="{{action}}"
  url="{{url}}"
  manialink="{{manialink}}"
  scriptaction="{{scriptAction}}"
  style="{{style}}"
  textfont="{{textFont}}"
  textsize="{{textSize}}"
  textcolor="{{textColor}}"
  focusareacolor1="{{focusAreaColor1}}"
  focusareacolor2="{{focusAreaColor2}}"
  text="{{text}}"
  textprefix="{{textPrefix}}"
  textemboss="{{boolToNum textEmboss}}"
  autonewline="{{boolToNum autoNewLine}}"
  linespacing="{{lineSpacing}}"
  maxline="{{maxLine}}"
  translate="{{boolToNum translate}}"
  textid="{{textId}}"
  appendellipsis="{{boolToNum appendEllipsis}}"
  italicslope="{{italicSlope}}"
/>
